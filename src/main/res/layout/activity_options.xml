<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.bansal.all_in_one_ipec.Options"
    android:background="@drawable/background4">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Everything Is Here"
        android:id="@+id/textView2"
        android:textSize="42dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:textColor="@color/abc_background_cache_hint_selector_material_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Canteen Orders"
        android:id="@+id/button5"
        android:layout_marginTop="30dp"
        android:textSize="25dp"
        android:textColor="#d71212"
        android:background="@color/bright_foreground_disabled_material_light"
        android:layout_below="@+id/textView2"
        android:layout_alignEnd="@+id/button6"
        android:layout_alignStart="@+id/button6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Event Notification"
        android:id="@+id/button6"
        android:layout_marginTop="25dp"
        android:textSize="25dp"
        android:background="@color/bright_foreground_disabled_material_light"
        android:textColor="#d71212"
        android:layout_below="@+id/button5"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOST AND FOUNd"
        android:id="@+id/button7"
        android:layout_marginTop="27dp"
        android:textSize="25dp"
        android:textColor="#d71212"
        android:background="@color/bright_foreground_disabled_material_light"
        android:layout_below="@+id/button6"
        android:layout_alignEnd="@+id/button6"
        android:layout_alignStart="@+id/button6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complain"
        android:id="@+id/button8"
        android:layout_marginTop="30dp"
        android:textSize="25dp"
        android:background="@color/bright_foreground_disabled_material_light"
        android:textColor="#d71212"
        android:layout_below="@+id/button7"
        android:layout_alignEnd="@+id/button7"
        android:layout_alignStart="@+id/button7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:id="@+id/button9"
        android:layout_below="@+id/button8"
        android:layout_marginTop="32dp"
        android:layout_alignEnd="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:textColor="#d71212"
        android:textSize="25dp"
        android:background="@color/bright_foreground_disabled_material_light" />
</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bansal.all_in_one_ipec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".Options" />
        <activity android:name=".SignUp" />
        <activity android:name=".CanteenOrders" />
        <activity android:name=".EventNotification" />
        <activity android:name=".LostAndFound" />
        <activity android:name=".Cleanliness" />
        <activity android:name=".ComplainPreview" />
        <activity android:name=".About"></activity>
    </application>

</manifest>